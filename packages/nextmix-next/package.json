{
  "name": "nextmix-next",
  "version": "1.0.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "next",
    "remix",
    "react",
    "adapter"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "nextmix-shared": "workspace:^"
  },
  "peerDependencies": {
    "next": ">=14",
    "react": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@remix-run/react": "*",
    "@types/node": "^20.12.12",
    "@types/react": "*",
    "mitt": "^3.0.0",
    "next": "*",
    "nextmix": "workspace:*",
    "react": "*",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6"
  }
}
