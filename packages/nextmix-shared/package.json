{
  "name": "nextmix-shared",
  "version": "0.2.3",
  "type": "commonjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "next",
    "remix",
    "react",
    "adapter"
  ],
  "scripts": {
    "dev": "pnpm run build --watch",
    "build": "tsup ./src/index.ts --dts --format esm,cjs",
    "prepublish": "pnpm run build"
  },
  "peerDependencies": {
    "@remix-run/react": ">=2",
    "next": ">=14",
    "react": "*"
  },
  "peerDependenciesMeta": {
    "@remix-run/react": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.39.1",
    "@remix-run/react": "*",
    "@types/node": "^20.12.12",
    "@types/react": "*",
    "mitt": "^3.0.0",
    "next": "*",
    "react": "*",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6"
  }
}
